{
    "name": "minimalic/silverstripe-bootloader",
    "description": "Bootstrap extension for custom 'main' theme for Silverstripe CMS 5",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "bootstrap",
        "theme",
        "css-framework"
    ],
    "authors": [
        {
            "name": "Sebastian Finke", "email": "dev@minimalic.com"
        }
    ],
    "require": {
        "silverstripe/cms": "^4 || ^5"
    },
    "autoload": {
        "psr-4": {
            "minimalic\\BootLoader\\": "src/"
        }
    },
    "extra": {
        "expose": [
            "client/dist"
        ]
    },
    "license": "BSD-3-Clause",
    "minimum-stability": "dev",
    "prefer-stable": true
}
